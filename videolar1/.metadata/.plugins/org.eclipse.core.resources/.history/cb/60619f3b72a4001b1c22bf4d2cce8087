package mukemmelSayi;

public class Main {
	public static void main(String[] args) {
		int sayi = 9;
		int toplam = 0;
		for(int i = 1; i<sayi; i++) {
			if(sayi %i == 0) {
				 toplam =+ i;
			}
		}
			if(sayi == toplam) {
				System.out.println("Mükemmel Sayýdýr");
				
			}else {
				System.out.println("Mükemmel sayý deðildir");
			}
		}
	}


