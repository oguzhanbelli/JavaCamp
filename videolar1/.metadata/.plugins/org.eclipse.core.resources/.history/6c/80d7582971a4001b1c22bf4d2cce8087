package sesliHarfler;

public class Main {
	public static void main(String[] args) {
		char harf = 'c';
		
	   switch(harf) {
	   
	   case 'A':
	   case'I':
	   case'O':
	   case'U':
		   System.out.println("Kalýn Sesli harf");
		   break;
		   default:
			   System.out.println("Ýnce Sesli Harf");
	   }
	   
	   
		
	
		
		
	}

}
