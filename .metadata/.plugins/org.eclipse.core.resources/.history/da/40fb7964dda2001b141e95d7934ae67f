		if (dolarBugun < dolarDun) {
			System.out.println("Dolar Düþmüþ");
			dustuMu = true;
			System.out.println(dustuMu);
		} else if (dolarBugun > dolarDun) {
			System.out.println("Dolar Artmýþ");
			dustuMu = false;
			System.out.println(dustuMu);

		}

		else {

			System.out.println("Dolar Eþittir");

		}
		String kredi1 = "Hýzlý Kredi";
