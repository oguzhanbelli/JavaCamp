package recapDemo1;

public class Main {
	public static void main(String[] args) {
		int sayi1 = 20;
		int sayi2= 25;
		int sayi3 = 2;
		if(sayi1 > sayi2 && sayi1 > sayi3) {
			
			System.out.println("Sayý 1 büyüktür");
			
		}else if(sayi2 > sayi1 && sayi2 > sayi3) {
			System.out.println("Sayý 2 büyüktür");
		}else if(sayi3 > sayi2 && sayi3 > sayi2) {
			System.out.println("Sayý 3 büyüktür");
		}
	}
	

}
